	<queryString language="SQL">
		<![CDATA[SELECT drogaria3.produto.descricao AS pDescricao,
	drogaria3.produto.preco,
	drogaria3.produto.quantidade,
	drogaria3.fabricante.descricao AS fDescricao
FROM drogaria3.produto
	INNER JOIN drogaria3.fabricante ON 
	 drogaria3.produto.fabricante_codigo = drogaria3.fabricante.codigo 
WHERE 
	 drogaria3.produto.descricao LIKE $P{PRODUTO_DESCRICAO} 
	 AND drogaria3.fabricante.descricao LIKE $P{FABRICANTE_DESCRICAO} 
ORDER BY drogaria3.produto.descricao ASC,
	drogaria3.fabricante.descricao ASC]]>
